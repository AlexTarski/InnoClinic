{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {TraceId} {Level:u3}] ({SourceContext}) {Message:lj}{NewLine}{Exception}",
          "theme": "Serilog.Sinks.SystemConsole.Themes.SystemConsoleTheme::Colored, Serilog.Sinks.Console"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName" ]
  },
  "AwsSettings": {
    "Endpoint": "AWS_S3_STORAGE_URL",
    "AccessKey": "AWS_S3_STORAGE_USER_NAME",
    "SecretKey": "AWS_S3_STORAGE_PASSWORD",
    "BucketName": "YOUR_BUCKET_NAME",
    "PhotoDoctorPath": "DIRECTORY_PATH",
    "PhotoReceptionistPath": "DIRECTORY_PATH",
    "PhotoPatientPath": "DIRECTORY_PATH",
    "PhotoOfficePath":  "DIRECTORY_PATH"
  },
  "ConnectionStrings": {
    "DocumentsDb": "DB_URL",
    "MinIoUrl": "STORAGE_URL",
    "AuthUrl": "AUTH_SERVER_URL"
  },
  "AllowedHosts": "*"
}