{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {TraceId} {Level:u3}] ({SourceContext}) {Message:lj}{NewLine}{Exception}",
          "theme": "Serilog.Sinks.SystemConsole.Themes.SystemConsoleTheme::Colored, Serilog.Sinks.Console"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName" ]
  },
  "ConnectionStrings": {
    "AuthorizationDb": "DB_CONNECTION_STRING"
  },
  "AppUrls": {
    "AuthUrl": "AUTHORIZATION_URL",
    "ProfilesUrl": "PROFILES_API_URL",
    "OfficesUrl": "OFFICES_API_URL",
    "EmployeeUiUrl": "EMPLOYEE_UI_URL",
    "ClientUiUrl": "CLIENT_UI_URL"
  },
  "EmailSettings": {
    "From": "FROM_USER_EMAIL",
    "DisplayName": "DISPLAYED_USER_NAME",
    "SmtpHost": "HOST_ADDRESS",
    "SmtpPort": 111,
    "CredUserName": "ACTUAL_USER_EMAIL",
    "CredPassword": "SMTP_SERVER_PASSWORD"
  },
  "AllowedHosts": "*"
}